{
    "npm-modules": {
        "key": "./npm-shrinkwrap.canonical.json",
        "cache": "../node_modules",
        "generate": "./uncached-npm-install.sh"
    },
    "rubygems": {
        "key": "../pseudo_Gemfile.lock",
        "cache": "$GEMDIR",
        "generate": "gem install -N jekyll -v $JEKYLL_VERSION"
    }
}
