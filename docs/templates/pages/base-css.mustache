  <!-- Code -->

  <h2>{{_i}}Code <small>Inline and block</small>{{/i}}</h2>
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th style="width: 190px;">{{_i}}Element{{/i}}</th>
        <th>{{_i}}Result{{/i}}</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><code>&lt;code&gt;</code></td>
